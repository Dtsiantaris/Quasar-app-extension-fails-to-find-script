{
  "name": "playground-quasar-cli-vite",
  "version": "0.0.1",
  "description": "Playground for Quasar AE blazar",
  "productName": "Quasar AE blazar Playground",
  "author": "Dimitris Tsiantaris <dtsiantaris@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "update-lib": "pnpm add -D ../../app-extension",
    "dev-restart": "quasar ext invoke @namespace/blazar && pnpm dev"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.15",
    "vite": "^6.0.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@namespace/quasar-app-extension-blazar": "link:..\\..\\app-extension",
    "@quasar/app-vite": "2.0.1",
    "@types/node": "^20.12.8",
    "autoprefixer": "^10.4.19",
    "moment-timezone": "^0.5.45",
    "postcss": "^8.4.38",
    "quasar": "2.17.1",
    "tailwindcss": "^3.4.3",
    "vue": "3.5.13"
  },
  "engines": {
    "node": "^20 || ^18"
  }
}
